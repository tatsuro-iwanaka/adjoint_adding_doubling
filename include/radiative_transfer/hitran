	Molecule(12, "HNO3", "HNO3"),
	Molecule(13, "OH", "OH"),
	Molecule(14, "HF", "HF"),
	Molecule(15, "HCl", "HCl"),
	Molecule(16, "HBr", "HBr"),
	Molecule(17, "HI", "HI"),
