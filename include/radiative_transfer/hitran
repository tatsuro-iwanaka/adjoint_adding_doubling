	Molecule(20, "H2CO", "H2CO"),
	Molecule(21, "HOCl", "HOCl"),
	Molecule(22, "N2", "N2"),
	Molecule(23, "HCN", "HCN"),
	Molecule(24, "CH3Cl", "CH3Cl"),
	Molecule(25, "H2O2", "H2O2"),
	Molecule(26, "C2H2", "C2H2"),
	Molecule(27, "C2H6", "C2H6"),
	Molecule(28, "PH3", "PH3"),
	Molecule(29, "COF2", "COF2"),
	Molecule(30, "SF6", "SF6"),
	Molecule(31, "H2S", "H2S"),
	Molecule(32, "HCOOH", "HCOOH"),
	Molecule(33, "HO2", "HO2"),
	Molecule(34, "O", "O"),
	Molecule(35, "ClONO2", "ClONO2"),
