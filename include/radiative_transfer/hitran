			return 0.23230087411688914593E0 + (-0.75653060136384041587E-2 + (0.21524991113020016415E-4 + (0.26388338542539382413E-6 + (-0.46368974069671446622E-8 + (0.40492715758206515307E-10 - 0.21238627815111111111E-12 * t) * t) * t) * t) * t) * t;
		}
		case 83:
		{
			double t = 2.0 * y100 - 167.0;
			return 0.21725840021297341931E0 + (-0.74761846305979730439E-2 + (0.23000194404129495243E-4 + (0.22837400135642906796E-6 + (-0.42446743058417541277E-8 + (0.37958104071765923728E-10 - 0.20963978568888888889E-12 * t) * t) * t) * t) * t) * t;
		}
		case 84:
		{
			double t = 2.0 * y100 - 169.0;
			return 0.20239979200788191491E0 + (-0.73815761980493466516E-2 + (0.24271552727631854013E-4 + (0.19590154043390012843E-6 + (-0.38775884642456551753E-8 + (0.35470192372162901168E-10 - 0.20470131678222222222E-12 * t) * t) * t) * t) * t) * t;
		}
		case 85:
		{
			double t = 2.0 * y100 - 171.0;
			return 0.18773523211558098962E0 + (-0.72822604530339834448E-2 + (0.25356688567841293697E-4 + (0.16626710297744290016E-6 + (-0.35350521468015310830E-8 + (0.33051896213898864306E-10 - 0.19811844544000000000E-12 * t) * t) * t) * t) * t) * t;
		}
		case 86:
		{
			double t = 2.0 * y100 - 173.0;
			return 0.17327341258479649442E0 + (-0.71789490089142761950E-2 + (0.26272046822383820476E-4 + (0.13927732375657362345E-6 + (-0.32162794266956859603E-8 + (0.30720156036105652035E-10 - 0.19034196304000000000E-12 * t) * t) * t) * t) * t) * t;
		}
		case 87:
		{
			double t = 2.0 * y100 - 175.0;
			return 0.15902166648328672043E0 + (-0.70722899934245504034E-2 + (0.27032932310132226025E-4 + (0.11474573347816568279E-6 + (-0.29203404091754665063E-8 + (0.28487010262547971859E-10 - 0.18174029063111111111E-12 * t) * t) * t) * t) * t) * t;
		}
		case 88:
		{
			double t = 2.0 * y100 - 177.0;
			return 0.14498609036610283865E0 + (-0.69628725220045029273E-2 + (0.27653554229160596221E-4 + (0.92493727167393036470E-7 + (-0.26462055548683583849E-8 + (0.26360506250989943739E-10 - 0.17261211260444444444E-12 * t) * t) * t) * t) * t) * t;
		}
		case 89:
		{
			double t = 2.0 * y100 - 179.0;
			return 0.13117165798208050667E0 + (-0.68512309830281084723E-2 + (0.28147075431133863774E-4 + (0.72351212437979583441E-7 + (-0.23927816200314358570E-8 + (0.24345469651209833155E-10 - 0.16319736960000000000E-12 * t) * t) * t) * t) * t) * t;
		}
		case 90:
		{
			double t = 2.0 * y100 - 181.0;
			return 0.11758232561160626306E0 + (-0.67378491192463392927E-2 + (0.28525664781722907847E-4 + (0.54156999310046790024E-7 + (-0.21589405340123827823E-8 + (0.22444150951727334619E-10 - 0.15368675584000000000E-12 * t) * t) * t) * t) * t) * t;
		}
		case 91:
		{
			double t = 2.0 * y100 - 183.0;
			return 0.10422112945361673560E0 + (-0.66231638959845581564E-2 + (0.28800551216363918088E-4 + (0.37758983397952149613E-7 + (-0.19435423557038933431E-8 + (0.20656766125421362458E-10 - 0.14422990012444444444E-12 * t) * t) * t) * t) * t) * t;
		}
		case 92:
		{
			double t = 2.0 * y100 - 185.0;
			return 0.91090275493541084785E-1 + (-0.65075691516115160062E-2 + (0.28982078385527224867E-4 + (0.23014165807643012781E-7 + (-0.17454532910249875958E-8 + (0.18981946442680092373E-10 - 0.13494234691555555556E-12 * t) * t) * t) * t) * t) * t;
		}
		case 93:
		{
			double t = 2.0 * y100 - 187.0;
			return 0.78191222288771379358E-1 + (-0.63914190297303976434E-2 + (0.29079759021299682675E-4 + (0.97885458059415717014E-8 + (-0.15635596116134296819E-8 + (0.17417110744051331974E-10 - 0.12591151763555555556E-12 * t) * t) * t) * t) * t) * t;
		}
		case 94:
		{
			double t = 2.0 * y100 - 189.0;
			return 0.65524757106147402224E-1 + (-0.62750311956082444159E-2 + (0.29102328354323449795E-4 + (-0.20430838882727954582E-8 + (-0.13967781903855367270E-8 + (0.15958771833747057569E-10 - 0.11720175765333333333E-12 * t) * t) * t) * t) * t) * t;
		}
		case 95:
		{
			double t = 2.0 * y100 - 191.0;
			return 0.53091065838453612773E-1 + (-0.61586898417077043662E-2 + (0.29057796072960100710E-4 + (-0.12597414620517987536E-7 + (-0.12440642607426861943E-8 + (0.14602787128447932137E-10 - 0.10885859114666666667E-12 * t) * t) * t) * t) * t) * t;
		}
		case 96:
		{
			double t = 2.0 * y100 - 193.0;
			return 0.40889797115352738582E-1 + (-0.60426484889413678200E-2 + (0.28953496450191694606E-4 + (-0.21982952021823718400E-7 + (-0.11044169117553026211E-8 + (0.13344562332430552171E-10 - 0.10091231402844444444E-12 * t) * t) * t) * t) * t) * t;
		}
		case 97: case 98: case 99: case 100:
		{ // use Taylor expansion for small x (|x| <= 0.0309...) (2/sqrt(pi)) * (x - 2/3 x^3  + 4/15 x^5  - 8/105 x^7 + 16/945 x^9) 
			double x2 = x * x;
			return x * (1.1283791670955125739 - x2 * (0.75225277806367504925 - x2 * (0.30090111122547001970 - x2 * (0.085971746064420005629 - x2 * 0.016931216931216931217))));
		}
	}
	/* Since 0 <= y100 < 101, this is only reached if x is NaN,
		in which case we should return NaN. */
	return std::numeric_limits<double>::quiet_NaN();
}

inline double w_im(double x)
{
	if (x >= 0.0)
	{
		if (x > 45.0)
		{
			const double ispi = 0.56418958354775628694807945156;
			
			if (x > 5.0E7)
			{
				return ispi / x;
			}
			
			return ispi * ((x * x) * (x * x - 4.5) + 2.0) / (x * ((x * x) * (x * x - 5.0) + 3.75));
		}
		
		return w_im_y100(100.0 / (1.0 + x), x);
	}
	else
	{
		if (x < -45.0)
		{
			const double ispi = 0.56418958354775628694807945156;
			
			if (x < -5.0E7)
			{
				return ispi / x;
			}
			
			return ispi * ((x * x) * (x * x - 4.5) + 2.0) / (x * ((x * x) * (x * x - 5.0) + 3.75));
		}

		return -w_im_y100(100.0 / (1.0 - x), -x);
	}
}

inline double erfcx(double x)
{
	if (x >= 0.0)
	{
		if (x > 50.0)
		{
			const double ispi = 0.56418958354775628694807945156; // 1/sqrt(pi)

			if (x > 5.0E7)
			{
				return ispi / x;
			}

			return ispi * ((x * x) * (x * x + 4.5) + 2.0) / (x * ((x * x) * (x * x + 5.0) + 3.75));
		}

		return erfcx_y100(400.0 / (4.0 + x));
	}
	else 
	{
		return x < -26.7 ? HUGE_VAL : (x < -6.1 ? 2.0 * std::exp(x * x) : 2.0 * std::exp(x * x) - erfcx_y100(400.0 / (4.0 - x)));
	}
}

inline std::complex<double> computeFaddeevaFunction(std::complex<double> z, double relerr = 0.0)
{
	if (std::real(z) == 0.0)
	{
		return std::complex<double>(erfcx(std::imag(z)), std::real(z));
	}
	else if (std::imag(z) == 0.0)
	{
		return std::complex<double>(std::exp(-sqr(std::real(z))), w_im(std::real(z)));
	}

	double a, a2, c;
	
	if (relerr <= DBL_EPSILON)
	{
		relerr = DBL_EPSILON;

		a = 0.518321480430085929872;
		c = 0.329973702884629072537;
		a2 = 0.268657157075235951582;
	}
	else
	{
		const double pi = 3.14159265358979323846264338327950288419716939937510582;

		if (relerr > 0.1)
		{
			relerr = 0.1;
		}

		a = pi / std::sqrt(-std::log(relerr * 0.5));
		c = (2.0 / pi) * a;
		a2 = a * a;
	}

	const double x = std::fabs(std::real(z));
	const double y = std::imag(z);
	const double ya = std::fabs(y);

	std::complex<double> ret = 0.0;

	double sum1 = 0.0;
	double sum2 = 0.0;
	double sum3 = 0.0;
	double sum4 = 0.0;
	double sum5 = 0.0;
	
	if (ya > 7.0 || (x > 6.0 && (ya > 0.1 || (x > 8.0 && ya > 1.0E-10) || x > 28.0)))
	{
		const double ispi = 0.56418958354775628694807945156; // 1/sqrt(pi)
		double xs = y < 0.0 ? -std::real(z) : std::real(z);

		if(x + ya > 4000.0)
		{
			if(x + ya > 1.0E7)
			{
				if(x > ya)
				{
					double yax = ya / xs;
					double denom = ispi / (xs + yax * ya);
					ret = std::complex<double>(denom * yax, denom);
				}
				else if(std::isinf(ya))
				{
					return ((std::isnan(x) || y < 0) ? std::complex<double>(std::numeric_limits<double>::quiet_NaN(), std::numeric_limits<double>::quiet_NaN()) : std::complex<double>(0,0));
				}
				else
				{
					double xya = xs / ya;
					double denom = ispi / (xya * xs + ya);
					ret = std::complex<double>(denom, denom * xya);
				}
			}
			else
			{
				double dr = xs * xs - ya * ya - 0.5;
				double di = 2.0 * xs * ya;
				double denom = ispi / (dr * dr + di * di);
				ret = std::complex<double>(denom * (xs * di - ya * dr), denom * (xs * dr + ya * di));
			}
		}
		else
		{
			const double c0 = 3.9;
			const double c1 = 11.398;
			const double c2 = 0.08254;
			const double c3 = 0.1421;
			const double c4 = 0.2023;

			double nu = std::floor(c0 + c1 / (c2 * x + c3 * ya + c4));
			double wr = xs;
			double wi = ya;

			for(nu = 0.5 * (nu - 1); nu > 0.4; nu -= 0.5)
			{
				double denom = nu / (wr * wr + wi * wi);
				wr = xs - wr * denom;
				wi = ya + wi * denom;
			}

			double denom = ispi / (wr * wr + wi * wi);
			ret = std::complex<double>(denom * wi, denom * wr);
		}

		if (y < 0.0)
		{
			return 2.0 * std::exp(std::complex<double>((ya - x) * (x + ya), 2.0 * xs * y)) - ret;
		}
		else
		{
			return ret;
		}
	}

	else if (x < 10.0) {
		double prod2ax = 1.0;
		double prodm2ax = 1.0;
		double expx2;

		if (std::isnan(y))
		{
			return std::complex<double>(y, y);
		}

		if (relerr == DBL_EPSILON)
		{
			if(x < 5.0E-4)
			{
				const double x2 = x * x;
				expx2 = 1.0 - x2 * (1.0 - 0.5 * x2);
				const double ax2 = 1.036642960860171859744 * x;

				const double exp2ax = 1.0 + ax2 * (1 + ax2 * (0.5 + 0.166666666666666666667 * ax2));
				const double expm2ax = 1.0 - ax2 * (1 - ax2 * (0.5 - 0.166666666666666666667 * ax2));

				for(int n = 1; 1; n ++)
				{
					const double coef = expa2n2[n - 1] * expx2 / (a2 * (n * n) + y * y);
					prod2ax *= exp2ax;
					prodm2ax *= expm2ax;

					sum1 += coef;
					sum2 += coef * prodm2ax;
					sum3 += coef * prod2ax;
					sum5 += coef * (2.0 * a) * n * sinh_taylor((2.0 * a) * n * x);
					
					if (coef * prod2ax < relerr * sum3)
					{
						break;
					}
				}
			}
			else
			{
				expx2 = std::exp(-x * x);
				const double exp2ax = std::exp((2.0 * a) * x);
				const double expm2ax = 1.0 / exp2ax;

				for (int n = 1; 1; n ++)
				{
					const double coef = expa2n2[n - 1] * expx2 / (a2 * (n * n) + y * y);
					prod2ax *= exp2ax;
					prodm2ax *= expm2ax;

					sum1 += coef;
					sum2 += coef * prodm2ax;
					sum4 += (coef * prodm2ax) * (a * n);
					sum3 += coef * prod2ax;
					sum5 += (coef * prod2ax) * (a * n);

					if ((coef * prod2ax) * (a * n) < relerr * sum5)
					{
						break;
					}
				}
			}
		}
		else
		{
			const double exp2ax = std::exp((2.0 * a) * x);
			const double expm2ax = 1.0 / exp2ax;

			if(x < 5.0E-4)
			{
				const double x2 = x * x;
				expx2 = 1.0 - x2 * (1.0 - 0.5 * x2);

				for (int n = 1; 1; n ++)
				{
					const double coef = std::exp(-a2 * (n * n)) * expx2 / (a2 * (n * n) + y * y);
					prod2ax *= exp2ax;
					prodm2ax *= expm2ax;

					sum1 += coef;
					sum2 += coef * prodm2ax;
					sum3 += coef * prod2ax;
					sum5 += coef * (2.0 * a) * n * sinh_taylor((2.0 * a) * n * x);

					if(coef * prod2ax < relerr * sum3)
					{
						break;
					}
				}
			}
			else
			{
				expx2 = std::exp(-x * x);

				for(int n = 1; 1; n ++)
				{
					const double coef = std::exp(-a2 * (n * n)) * expx2 / (a2 * (n * n) + y * y);
					prod2ax *= exp2ax;
					prodm2ax *= expm2ax;

					sum1 += coef;
					sum2 += coef * prodm2ax;
					sum4 += (coef * prodm2ax) * (a * n);
					sum3 += coef * prod2ax;
					sum5 += (coef * prod2ax) * (a * n);

					if((coef * prod2ax) * (a * n) < relerr * sum5)
					{
						break;
					}
				}
			}
		}
		const double expx2erfcxy = y > -6.0 ? expx2 * erfcx(y) : 2.0 * std::exp(y * y - x * x);

		if (y > 5.0)
		{
			const double sinxy = std::sin(x * y);
			ret = (expx2erfcxy - c * y * sum1) * std::cos(2.0 * x * y) + (c * x * expx2) * sinxy * sinc(x * y, sinxy);
		}
		else
		{
			double xs = std::real(z);
			const double sinxy = std::sin(xs * y);

			const double sin2xy = std::sin(2.0 * xs * y);
			const double cos2xy = std::cos(2.0 * xs * y);

			const double coef1 = expx2erfcxy - c * y * sum1;
			const double coef2 = c * xs * expx2;

			ret = std::complex<double>(coef1 * cos2xy + coef2 * sinxy * sinc(xs * y, sinxy), coef2 * sinc(2.0 * xs * y, sin2xy) - coef1 * sin2xy);
		}
	}
	else
	{
		ret = std::exp(-x * x);
	
		double n0 = std::floor(x / a + 0.5);
		double dx = a * n0 - x;
		
		sum3 = std::exp(-dx * dx) / (a2 * (n0 * n0) + y * y);
		sum5 = a * n0 * sum3;

		double exp1 = std::exp(4.0 * a * dx);
		double exp1dn = 1.0;
		int dn;

		for(dn = 1; n0 - dn > 0; dn ++)
		{
			double np = n0 + dn;
			double nm = n0 - dn;

			double tp = std::exp(-sqr(a * dn + dx));
			double tm = tp * (exp1dn *= exp1);

			tp /= (a2 * (np * np) + y * y);
			tm /= (a2 * (nm * nm) + y * y);
			sum3 += tp + tm;
			sum5 += a * (np * tp + nm * tm);

			if (a * (np * tp + nm * tm) < relerr * sum5)
			{
				goto finish;
			}
		}
		while(1)
		{
			double np = n0 + dn ++;
			double tp = std::exp(-sqr(a * dn + dx)) / (a2 * (np * np) + y * y);

			sum3 += tp;
			sum5 += a * np * tp;

			if(a * np * tp < relerr * sum5)
			{
				goto finish;
			}
		}
	}
	
	finish:

	return ret + std::complex<double>((0.5 * c) * y * (sum2 + sum3), (0.5 * c) * std::copysign(sum5 - sum4, std::real(z)));
}

}

namespace hitran
{
const double AVOGADRO_CONSTANT = 6.02214076E23;	//Avogadro constant (molecules/mol)
const double MOLAR_GAS_CONSTANT = 8.31446261815324;	//molar gas constant (J/K/mol)
const double PLANCK_CONSTANT = 6.62607015E-34; // J s
const double SPEED_OF_LIGHT = 2.99792458E8; // m s1
const double BOLTZMANN_CONSTANT = 1.380649E-23; // J K1

const double ATM_TO_PA = 101325.0; // atm -> Pa
const double CM_TO_M = 1.0E-2; // cm -> m
const double UM_TO_M = 1.0E-6; // um -> m
const double GAMMA_UNIT = 1.0 / (CM_TO_M * ATM_TO_PA); // cm-1/atm -> m-1/Pa

const double TREF = 296.0; // K

class Molecule
{
	public:
		int molecule_id; // : CO2 -> 2
		std::string name; // "CO2"
		std::string general_formula; // "CO2", "H2O"

		Molecule(void);
		Molecule(int, std::string, std::string);
};

class Isotopologue
{
	public:
		int global_id; // HITRAN global iso ID
		int molecule_id; // ID
		int local_iso_id; // local isotopologue ID
		std::string isotopic_formula; // : "12C16O2"
		int afgl_code; // HITRAN AFGL
		std::string general_formula; // : "CO2"
		double abundance; // 
		double molar_mass; // kg/mol
		int gi; // nuclear spin weight

		struct QTable
		{
			std::vector<double> T;
			std::vector<double> Q;
		};

		Isotopologue(void);
		Isotopologue(int, int, int, std::string, int, std::string, double, double, int);

		QTable qtable;
		void loadQTable(const netCDF::NcFile&);
		void loadQTable(const std::string&);
		double interpolateQ(double temperature) const;
};

