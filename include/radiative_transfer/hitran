namespace faddeeva
{
std::complex<double> computeFaddeevaFunction(std::complex<double>, double);

inline double sinc(double x, double sinx)
{
	return std::fabs(x) < 1.0E-4 ? 1.0 - (1.0 / 6.0) * x * x : sinx / x;
}

inline double sinh_taylor(double x)
{
	return x * (1 + (x * x) * (1.0 / 6.0 + (1.0 / 120.0) * (x * x)));
}

inline double sqr(double x)
{
	return x * x;
}

